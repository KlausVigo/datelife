setClass(Class = "ClassName",
    representation(

    )
)

#' Main DateLife object. It stores results from a DateLife search
#' @rdname DateLife
#' @export

setClass(Class = "DateLife",
    representation(
        query = "character",
        citations = " character",
        patristic_distances = "matrix",
        missing_taxa = "matrix"
    )
)

#' Stores information on a processed DateLife Query.
#' @rdname DateLifeQuery
#' @export

setClass(Class = "DateLifeQuery",
    representation(
        phy = "phylo",
        cleaned_names = "character",
        name_database = "character"
    )
)

#' Main DateLife object. It stores results from a DateLife search
#' @rdname DateLifeQueryMissingTaxa
#' @export

setClass(Class = "MissingTaxa",
    representation(
        
    )
)
