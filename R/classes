# Defines classes of object resulting from DateLife main methods.

setClass(Class = "ClassName",
    representation(

    )
)

#' Main DateLife object. It stores results from a DateLife search.
#' @rdname DateLife
#' @export

setClass(Class = "DateLife",
    representation(
        citations = " character",
        patristic_distances = "matrix",
        missing_taxa = "matrix"
    )
)

#' DateLife query object. It stores information on a query that has been processed.
#' @rdname DateLifeQuery
#' @export

setClass(Class = "DateLifeQuery",
    representation(
        phy = "phylo",
        cleaned_names = "character",
        name_database = "character"
    )
)
